def solve_stonebridge_primes(x, y):
    prime_counter = 0
    m, n = 0, 0
    
    while True:
        number = (2**m)*(3**n) + 1
        if number in primes:
            if number >= x and number < y:
                    prime_counter += 1
            
        if (2**(m+1)) + 1 >= y:
            m = 0
            n += 1
            
        elif number < y or (3**(n+1) + 1) < number:
            m += 1
            
        else:
            break
            
    return prime_counter

